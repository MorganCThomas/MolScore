{
  "task": "Mestranol_similarity",
  "output_dir": "./",
  "load_from_previous": false,
  "logging": false,
  "monitor_app": false,
  "diversity_filter": {
    "run": false
  },
  "scoring_functions": [
    {
      "name": "MolecularSimilarity",
      "run": true,
      "parameters": {
        "prefix": "Mestranol",
        "ref_smiles": [
          "CC12CCC3C(C1CCC2(C#C)O)CCC4=C3C=CC(=C4)OC"
        ],
        "fp": "AP",
        "bits": 1024,
        "similarity_measure": "Tanimoto",
        "method": "mean",
        "n_jobs": 1
      }
    }
  ],
  "scoring": {
    "method": "single",
    "metrics": [
      {
        "name": "Mestranol_Sim",
        "weight": 1.0,
        "modifier": "lin_thresh",
        "parameters": {
          "objective": "maximize",
          "upper": 0.75,
          "lower": 0.0,
          "buffer": 0.75
        }
      }
    ]
  }
}
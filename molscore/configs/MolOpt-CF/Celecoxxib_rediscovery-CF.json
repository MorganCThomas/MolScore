{
  "task": "Celecoxxib_rediscovery",
  "output_dir": "./",
  "load_from_previous": false,
  "logging": false,
  "monitor_app": false,
  "diversity_filter": {
    "run": true,
    "name": "ScaffoldSimilarityECFP",
    "parameters": {
      "nbmax": 50,
      "minscore": 0.5,
      "minsimilarity": 0.8,
      "radius": 2,
      "useFeatures": false,
      "bits": 1024,
      "outputmode": "linear"
    }
  },
  "scoring_functions": [
    {
      "name": "TanimotoSimilarity",
      "run": true,
      "parameters": {
        "prefix": "Celecoxxib",
        "ref_smiles": [
          "Cc1ccc(-c2cc(C(F)(F)F)nn2-c2ccc(S(N)(=O)=O)cc2)cc1"
        ],
        "radius": 2,
        "bits": 1024,
        "features": false,
        "counts": true,
        "method": "max",
        "n_jobs": 1
      }
    },
    {
      "name": "ChemistryFilter",
      "run": true,
      "parameters": {
        "ref_smiles": "/workspace1/Priors/ChEMBL_potent/processed_data/ChEMBL28p_all_undersample-8.smi.gz",
        "n_jobs": 4
      }
    }
  ],
  "scoring": {
    "method": "single",
    "metrics": [
      {
        "name": "Celecoxxib_Sim",
        "weight": 1.0,
        "modifier": "raw",
        "parameters": {}
      },
      {
        "name": "B&T_CF",
        "filter": true,
        "modifier": "raw",
        "weight": 1.0,
        "parameters": {}
      }
    ]
  }
}
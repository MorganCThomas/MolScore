{
  "task": "Zaleplon_MPO",
  "output_dir": "./",
  "load_from_previous": false,
  "logging": false,
  "monitor_app": false,
  "diversity_filter": {
    "run": true,
    "name": "ScaffoldSimilarityECFP",
    "parameters": {
      "nbmax": 50,
      "minscore": 0.5,
      "minsimilarity": 0.8,
      "radius": 2,
      "useFeatures": false,
      "bits": 1024,
      "outputmode": "linear"
    }
  },
  "scoring_functions": [
    {
      "name": "TanimotoSimilarity",
      "run": true,
      "parameters": {
        "prefix": "Zaleplon",
        "ref_smiles": [
          "CCN(C(C)=O)c1cccc(-c2ccnc3c(C#N)cnn23)c1"
        ],
        "radius": 2,
        "bits": 1024,
        "features": false,
        "counts": true,
        "method": "max",
        "n_jobs": 1
      }
    },
    {
      "name": "Isomer",
      "run": true,
      "parameters": {
        "prefix": "C19H17N3O2",
        "molecular_formula": "C19H17N3O2"
      }
    },
    {
      "name": "ChemistryFilter",
      "run": true,
      "parameters": {
        "ref_smiles": "/workspace1/Priors/ChEMBL_potent/processed_data/ChEMBL28p_all_undersample-8.smi.gz",
        "n_jobs": 4
      }
    }
  ],
  "scoring": {
    "method": "gmean",
    "metrics": [
      {
        "name": "Zaleplon_Sim",
        "weight": 1.0,
        "modifier": "raw",
        "parameters": {}
      },
      {
        "name": "C19H17N3O2_isomer_score",
        "weight": 1.0,
        "modifier": "raw",
        "parameters": {}
      },
      {
        "name": "B&T_CF",
        "filter": true,
        "modifier": "raw",
        "weight": 1.0,
        "parameters": {}
      }
    ]
  }
}
{
  "task": "Amlodipine_MPO",
  "output_dir": "./",
  "load_from_previous": false,
  "logging": false,
  "monitor_app": false,
  "diversity_filter": {
    "run": false
  },
  "scoring_functions": [
    {
      "name": "TanimotoSimilarity",
      "run": true,
      "parameters": {
        "prefix": "Amlodipine",
        "ref_smiles": [
          "CCOC(=O)C1=C(COCCN)NC(C)=C(C(=O)OC)C1c1ccccc1Cl"
        ],
        "radius": 2,
        "bits": 1024,
        "features": false,
        "counts": true,
        "method": "max",
        "n_jobs": 1
      }
    },
    {
      "name": "RDKitDescriptors",
      "run": true,
      "parameters": {
        "prefix": "desc",
        "n_jobs": 1
      }
    }
  ],
  "scoring": {
    "method": "gmean",
    "metrics": [
      {
        "name": "Amlodipine_Sim",
        "weight": 1.0,
        "modifier": "raw",
        "parameters": {}
      },
      {
        "name": "desc_RingCount",
        "weight": 1.0,
        "modifier": "gauss",
        "parameters": {
          "objective": "range",
          "mu": 3.0,
          "sigma": 0.5
        }
      }
    ]
  }
}